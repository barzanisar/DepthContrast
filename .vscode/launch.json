{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: extract splits",
            "type": "python",
            "request": "launch",
            "program": "extract_splits.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/data/dense"
        },
